{
  "multipart": [
    {
      "apply": {
        "model": "dtbop:block/palm_leaves/palm_frond"
      },
      "when": {
        "distance": "1|2"
      }
    },
    {
      "apply": {
        "model": "dtbop:block/palm_leaves/palm_core_top"
      },
      "when": {
        "distance": "3"
      }
    },
    {
      "apply": {
        "model": "dtbop:block/palm_leaves/palm_core_bottom"
      },
      "when": {
        "distance": "4"
      }
    },
    {
      "apply": {
        "model": "biomesoplenty:block/palm_leaves"
      },
      "when": {
        "OR": [
          {
            "direction": "0",
            "distance": "1|2"
          },
          {
            "distance": "5|6|7"
          }
        ]
      }
    }
  ]
}